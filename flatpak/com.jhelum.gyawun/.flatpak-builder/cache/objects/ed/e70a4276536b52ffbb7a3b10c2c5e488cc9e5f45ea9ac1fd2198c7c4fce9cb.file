[Application]
name=com.jhelum.gyawun
runtime=org.freedesktop.Platform/x86_64/24.08
sdk=org.freedesktop.Sdk/x86_64/24.08
command=gyawun

[Context]
shared=network;
sockets=x11;wayland;
filesystems=host;

[Extension com.jhelum.gyawun.Debug]
directory=lib/debug
autodelete=true
no-autodownload=true

[Build]
built-extensions=com.jhelum.gyawun.Debug;